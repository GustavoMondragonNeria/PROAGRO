@model List<ServicioWebDominio.spUbicacionesGeorreferenciasPorUsuarioExtDominio>
@{
    Layout = null;

    <!DOCTYPE html>
}
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>PROAGRO</title>

    <link rel="icon" type="image/png" href="~/Imagenes/PROAGRO.png" sizes="32x32" />
    @*<img src="~/Imagenes/PROAGRO.png" />*@
    <link href="../Content/Registro.css" rel="stylesheet" />
    <link href="~/Content/Contenido/vendor/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <!-- Theme CSS -->
    <link href="~/Content/Contenido/stylesheets/theme.css" rel="stylesheet" />
    <!-- Skin CSS -->
    <link href="~/Content/Contenido/stylesheets/skins/default.css" rel="stylesheet" />
    <!-- Theme Custom CSS -->
    <link href="~/Content/Contenido/stylesheets/theme-custom.css" rel="stylesheet" />


</head>
<body>
    <div>
        <section class="body">
            @{
                //var usuario = (ServicioWebDominio.UsuarioExt)HttpContext.Current.Session["User"];
                //var nombre = usuario.Nombre;
                <!-- start: header -->
                @Html.Partial("_Layout_SuperUsuario")
                <!-- end: header -->

                <div class="inner-wrapper">
                    <!-- start: sidebar -->
                    <!-- end: sidebar -->

                    <section role="main" class="content-body">
                        <header class="page-header">
                            <h2>BIENVENIDO AL SISTEMA DE GEORREFERENCIAS </h2>

                            <div class="right-wrapper pull-right">
                            </div>
                        </header>

                        <table id="dataTableGeorreferencias" class="table table-striped table-hover" style="width:100%" cellspacing="0">
                            <thead>
                                <tr>
                                    <th hidden>ID</th>
                                    <th>NOMBRE</th>
                                    <th>FECHA</th>
                                    <th>RFC</th>
                                    <th>ESTADO</th>
                                    <th>ABREVIACIÓN</th>
                                    <th>LATITUD</th>
                                    <th>LONGITUD</th>

                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var r in Model)
                                {
                                    <tr>
                                        <td hidden class="txtID_USUARIO">@r.idUsuario</td>
                                        <td>@r.Nombre_USUARIO</td>
                                        <td>@Convert.ToDateTime(r.Fecha_Nacimiento).ToString("dd/MMMM/yyyy")</td>
                                        <td>@r.RFC</td>
                                        <td>@r.Nombre_ESTADOS</td>
                                        <td>@r.Abreviacion</td>
                                        <td>@r.Latitud</td>
                                        <td>@r.Longitud</td>

                                    </tr>

                                }
                            </tbody>
                            <tfoot>
                            </tfoot>
                        </table>
                        <br />
                        <br />
                        <a href="/ExamenCliente/Index" class="btn  btn-info btn-responsive"><i class="fas fa-backward fa-1x"></i>&nbsp;&nbsp;REGRESAR</a>
                        <button onclick="myfunction()" type="submit" class="btn btn-primary btn-responsive float-lg-right" id="Mapa"><i class="fas fa-eye"></i>&nbsp;&nbsp;MOSTRAR MAPA</button>

                        <center><img src="~/Imagenes/PROAGRO.png" class="img-circle" height="150" /></center>
                    </section>
                </div>

            }

        </section>
        <div class="float-right" id="map" style="height:500px; width:1050px; background:white; text-align:center;"></div>
        <br />
        <br />
        <br />
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD3oG7EFY36vpR0vCqaArdqy_SEuENzKEE&callback=initMap"></script>
        <script src="https://code.jquery.com/jquery-3.3.1.js"></script>
        <!-- Head Libs -->
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>
        <script src="https://kit.fontawesome.com/ed14086312.js"></script>
        <script src="../Content/Contenido/vendor/bootstrap/js/bootstrap.js"></script>
        <script src="../Content/Contenido/vendor/nanoscroller/nanoscroller.js"></script>
        <script src="../Content/Contenido/vendor/modernizr/modernizr.js"></script>
        <script src="../Content/Contenido/javascripts/theme.js"></script>
        <script src="~/JAVASCRIPT/GeorreferenciasPorUsuario.js"></script>
    </div>
</body>
</html>
